@font-face {
    font-family: "{{fontName}}";
    src: {{{src}}};
}

i {
    display: inline-block;
    min-width: 1em;
    line-height: 1;
    vertical-align: middle;
}

i[class*="{{classPrefix}}"]:before {
    font-family: {{fontName}} !important;
    font-style: normal;
    font-weight: normal !important;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
    content: "\\{{this}}";
}
{{/each}}
